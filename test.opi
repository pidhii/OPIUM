let l = 1:2:3:4:5:nil;
print ("l =", l);

let max =
  let max2 (x, y) = if x > y then x else y in
  let aux l =
    if let x:xs = l then foldl max2 x xs else l
  in aux . list;

print (max (1,2,3,4,5,6,7,8));

print "----------------------";
